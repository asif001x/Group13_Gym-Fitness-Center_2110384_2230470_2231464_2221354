<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.group13.asifur2110384.MembershipplanController">
    <children>
        <Label layoutX="198.0" layoutY="22.0" text="Member Ship Plan">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
        </Label>
        <Button layoutX="8.0" layoutY="9.0" mnemonicParsing="false" onAction="#BackButtonOnAction" text="Back" />
        <TableView layoutY="256.0" prefHeight="144.0" prefWidth="600.0">
            <columns>
                <TableColumn prefWidth="75.0" text="C1" />
                <TableColumn prefWidth="75.0" text="C2" />
            </columns>
        </TableView>
        <Button layoutX="534.0" layoutY="224.0" mnemonicParsing="false" onAction="#loginButtonOnAction" text="Log In" />
        <Label layoutX="14.0" layoutY="80.0" prefHeight="17.0" prefWidth="175.0" text="New Member Ship Plan  Name :" />
        <TextField fx:id="plannameTextField" layoutX="185.0" layoutY="76.0" promptText="Plan Name" />
        <Label layoutX="14.0" layoutY="156.0" prefHeight="17.0" prefWidth="129.0" text="Plan Ending  Date :" />
        <Label layoutX="14.0" layoutY="124.0" prefHeight="17.0" prefWidth="167.0" text="New Member Plan  price :" />
        <TextField fx:id="priceTextField" layoutX="185.0" layoutY="117.0" promptText="-----$$$$" />
        <DatePicker fx:id="dateDatePicker" layoutX="185.0" layoutY="152.0" prefHeight="25.0" prefWidth="149.0" promptText="Select Date" />
        <Button layoutX="123.0" layoutY="198.0" mnemonicParsing="false" onAction="#addButtonOnAction" text="ADD" />
        <Button layoutX="435.0" layoutY="111.0" mnemonicParsing="false" onAction="#currentButtonOnAction" text="Current Plan" />
        <Label fx:id="outputLabel" layoutX="260.0" layoutY="202.0" prefHeight="17.0" prefWidth="175.0" />
    </children>
</AnchorPane>
